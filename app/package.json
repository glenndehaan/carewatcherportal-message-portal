{
  "name": "carewatcherportal-message-portal",
  "version": "0.0.0",
  "description": "NPM packages for carewatcherportal-message-portal",
  "private": true,
  "scripts": {
    "start": "node ../app/server.js",
    "dev": "nodemon -L --watch ../app ../app/server.js",
    "docs": "apidoc -i ./ -o ../docs/ -e node_modules/"
  },
  "author": "Glenn de Haan",
  "license": "MIT",
  "dependencies": {
    "apidoc": "^0.17.6",
    "body-parser": "^1.17.2",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "multer": "^1.3.0",
    "node-json-db": "^0.7.3",
    "nodemon": "^1.13.3",
    "simple-node-logger": "^0.93.30",
    "socket.io": "^2.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "database.json"
    ]
  },
  "apidoc": {
    "name": "carewatcherportal-message-portal",
    "version": "0.0.0",
    "description": "Care Watcher Message Portal",
    "title": "Care Watcher Message Portal",
    "url" : "http://localhost:3001",
    "template": {
      "forceLanguage": "en"
    }
  }
